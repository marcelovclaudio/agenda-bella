{
  "name": "@agenda-bella/audit",
  "version": "0.1.0",
  "description": "Audit trail and LGPD compliance system for Agenda Bella",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "compliance:report": "tsx scripts/generate-compliance-report.ts"
  },
  "dependencies": {
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@agenda-bella/database": "workspace:*",
    "@agenda-bella/typescript": "workspace:*",
    "@types/node": "^20.8.9",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "tsup": "^7.2.0",
    "tsx": "^4.1.1",
    "typescript": "^5.2.2"
  }
}